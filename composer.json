{
    "name": "betasyntax/betasyntax",
    "description": "Beta Syntax Framework.",
    "keywords": ["framework", "betasyntax"],
    "license": "MIT",
    "type": "project",
    "version": "0.0.1",
    "minimum-stablilty":"dev",
    "require": {
        "php": ">=5.5.9",
        "ext-mbstring":"*",
        "ext-openssl":"*",
        "betasyntax/framework":"0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit":"5.4.*"
    },
    "autoload": {
        "psr-4": {
          "App\\":"app/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "mv -Rf ./vendor/betasyntax/betasyntax/* ./",
            "rm -Rf ./vendor/betasyntax/betasyntax",
            "composer update"
        ]
    }
}
