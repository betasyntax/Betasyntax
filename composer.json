{
    "name": "betasyntax/betasyntax",
    "description": "Beta Syntax Framework.",
    "keywords": ["framework", "betasyntax"],
    "license": "MIT",
    "type": "project",
    "version": "0.0.1",
    "minimum-stablilty":"dev",
    "require": {
        "php": ">=5.5.9",
        "ext-mbstring":"*",
        "ext-openssl":"*",
        "mnsami/composer-custom-directory-installer": "1.1.*",
        "betasyntax/framework":"0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit":"5.4.*"
    },
    "autoload": {
        "psr-4": {
          "App\\":"app/"
        }
    },
    "extra": {
        "installer-paths":{
          "./":["monolog/monolog"]
        }
    }
}
